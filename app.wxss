/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


.head{
  display: block;
  width:100%;
  height: 1.5rem;
  position:fixed;
  background:#F92246;
  z-index: 999;
}
.header{
  width:90%;
  height: 1.5rem;
  text-align: center;
  line-height: 1.5rem;
  float:left;
}
.header text{
  color:#fff;
  font-size: 0.8rem;
  font-weight:800;
}
.sousuo{
  width:10%;
  height: 1.5rem;
  float:right;
  line-height: 1.8rem;
  text-align: center;
}

/*footer*/
.footer{
  width:100%;
  background: #e4e5e9;
}
.footer text{
  display: block;
  width:100%;
  text-align: center;
  line-height:1.5rem;
}