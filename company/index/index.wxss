page{height: 100%;}

.content{font-family:"微软雅黑";}
.com_item{padding: 20rpx;border-bottom:1px solid #cccccc; }
/*header*/
.com_item_header{padding-left:106rpx;overflow:hidden;}
.com_item_header .com_img{margin-left: -106rpx;float: left;width: 90rpx;height:90rpx;overflow: hidden;border-radius: 50%;border:1px solid #cccccc;}
.com_item_header .com_img image{width: 90rpx;height: 90rpx;}
.com_item_header .com_title{font-size:32rpx;line-height:48rpx;height:48rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.com_item_header .com_btn{height: 40rpx;}
.com_item_header .com_btn navigator{display: inline-block;width: 40rpx;height: 40rpx;margin: 0 4rpx;}
.com_item_header .com_btn image{width: 40rpx;height: 40rpx;}
/*body*/
.com_item_body{font-size: 24rpx;color: #666666;margin: 20rpx 0 10rpx;}
.main_pro{padding-left: 110rpx;}
.main_pro .tit{margin-left: -110rpx;padding-right: 10rpx;float: left;}
.main_pro .con{overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;word-wrap: break-word;}
.tel{padding-left: 70rpx;}
.tel .tit{margin-left: -70rpx;padding-right: 10rpx;float: left;}
.tel .con{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;height: 36rpx;display:block;}
.address{padding-left: 70rpx;}
.address .tit{margin-left: -70rpx;padding-right: 10rpx;float: left;}
.address .con{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;height: 36rpx;display:block;}
/*footer*/
.com_item_footer{font-size: 24rpx;color: #999999;overflow: hidden;line-height: 36rpx;}
.com_item_footer .left{float: left;}
.com_item_footer .right{float: right;}
.foot_img {display: inline-block;height: 30rpx;line-height: 30rpx;}
.foot_img image{width:30rpx;height: 30rpx;float:left;margin:4rpx 4rpx 0;}

/*back_top*/
.back_top{width: 100rpx;height: 100rpx;overflow: hidden;position: fixed;bottom: 80rpx;right: 20rpx;border-radius: 50%;}
.back_top image{width: 100rpx;height: 100rpx;}